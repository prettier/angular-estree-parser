{
  "name": "angular-estree-parser",
  "version": "10.0.2",
  "description": "A parser that converts Angular source code into an ESTree-compatible form",
  "keywords": [],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "https://github.com/prettier/angular-estree-parser",
  "homepage": "https://github.com/prettier/angular-estree-parser#readme",
  "author": {
    "name": "Ika",
    "email": "ikatyang@gmail.com",
    "url": "https://github.com/ikatyang"
  },
  "license": "MIT",
  "scripts": {
    "clean": "del-cli ./lib",
    "prepare": "yarn run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "run-p \"lint:*\"",
    "lint:eslint": "eslint --ext=.ts,.js,.cjs .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s \"fix:*\"",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "build": "yarn clean && tsc -p ./tsconfig.build.json",
    "release": "yarn build && standard-version"
  },
  "devDependencies": {
    "@angular/compiler": "18.0.0",
    "@babel/code-frame": "7.24.2",
    "@babel/parser": "7.24.5",
    "@babel/types": "7.24.5",
    "@types/babel__code-frame": "7.0.6",
    "@types/jest": "29.5.12",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "del-cli": "5.1.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-unicorn": "53.0.0",
    "jest": "29.7.0",
    "jest-snapshot-serializer-raw": "1.2.0",
    "lines-and-columns": "2.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "standard-version": "9.5.0",
    "ts-jest": "29.1.3",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "@angular/compiler": "^17.3.9 || ^18.0.0"
  },
  "engines": {
    "node": ">= 16"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "yarn@4.2.2"
}
