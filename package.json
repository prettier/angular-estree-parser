    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "62.0.0",
    "globals": "16.5.0",
    "jest-snapshot-serializer-raw": "2.0.0",
    "lines-and-columns": "2.0.4",
